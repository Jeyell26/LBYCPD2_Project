<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainMenuFX.LoginFX.LoginController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: EDE7e3;">
         <children>
            <Polygon fill="#f38701" layoutX="0.0" layoutY="0.0" stroke="#ff9b00" strokeType="INSIDE">
              <points>
                <Double fx:value="300.0" />
                <Double fx:value="0.0" />
                <Double fx:value="600.0" />
                <Double fx:value="0.0" />
                <Double fx:value="600.0" />
                <Double fx:value="400.0" />
                <Double fx:value="200.0" />
                <Double fx:value="400.0" />
              </points>
            </Polygon>
            <TextField fx:id="user" layoutX="312.0" layoutY="133.0" prefHeight="30.0" prefWidth="234.0" promptText="Enter your username">
               <font>
                  <Font size="9.0" />
               </font></TextField>
            <PasswordField fx:id="pass" layoutX="311.0" layoutY="185.0" prefHeight="30.0" prefWidth="234.0" promptText="Password">
               <font>
                  <Font size="9.0" />
               </font></PasswordField>
            <TextField fx:id="showPass" layoutX="311.0" layoutY="185.0" prefHeight="30.0" prefWidth="234.0" promptText="Enter your password">
               <font>
                  <Font size="9.0" />
               </font></TextField>
            <Button fx:id="login" layoutX="312.0" layoutY="258.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="233.0" style="-fx-background-color: f7a400;" text="LOGIN" textFill="WHITE">
               <font>
                  <Font name="Arial Narrow" size="14.0" />
               </font></Button>
            <Button fx:id="back" layoutX="311.0" layoutY="300.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="234.0" style="-fx-background-color: f7a400;" text="EXIT" textFill="WHITE">
               <font>
                  <Font name="Arial Narrow" size="14.0" />
               </font></Button>
            <Label layoutX="40.0" layoutY="186.0" text="Input Logo Here">
               <font>
                  <Font name="Calibri Bold" size="24.0" />
               </font>
            </Label>
            <Label layoutX="312.0" layoutY="119.0" text="USERNAME" textFill="#f4f4f9">
               <font>
                  <Font name="Calibri" size="10.0" />
               </font>
            </Label>
            <Label fx:id="error" layoutX="312.0" layoutY="83.0" prefHeight="42.0" prefWidth="234.0" textFill="#f4f4f9">
               <font>
                  <Font name="Calibri Bold" size="12.0" />
               </font>
            </Label>
            <CheckBox fx:id="showToggle" layoutX="312.0" layoutY="225.0" mnemonicParsing="false" text="Show Password" textFill="#f4f4f9">
               <font>
                  <Font size="9.0" />
               </font></CheckBox>
            <Label layoutX="312.0" layoutY="170.0" text="PASSWORD" textFill="#f4f4f9">
               <font>
                  <Font name="Calibri" size="10.0" />
               </font>
            </Label>
         </children></AnchorPane>
   </children>
</GridPane>
